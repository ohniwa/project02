FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet1
    {
        type            fixedValue;
        value           uniform (30 0 0);
    }

    inlet2
    {
        type            fixedValue;
        value           uniform (300 0 0);
    }

     outlet
    {
        type            zeroGradient;
    }
    
    axis
    {
        //type            zeroGradient;
        type            symmetryPlane;
    }
    
     wall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    frontAndBack
    {
        type            empty;
    }
}

